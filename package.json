{
  "name": "@ticatec/uniface-echarts",
  "version": "0.1.0",
  "description": "A lightweight Svelte wrapper for Apache ECharts with TypeScript support. / 一个轻量级的 ECharts Svelte 封装库，支持 TypeScript",
  "keywords": [
    "svelte",
    "echarts",
    "chart",
    "visualization",
    "typescript",
    "data-visualization",
    "web-components"
  ],
  "homepage": "https://github.com/ticatec/uniface-echarts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ticatec/uniface-echarts.git"
  },
  "bugs": {
    "url": "https://github.com/ticatec/uniface-echarts/issues"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepublishOnly": "npm run package",
    "publish:public": "npm run build && npm publish --access public"
  },
  "files": [
    "dist",
    "!dist/**/*.scss",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./ChartPanel": {
      "types": "./dist/ChartPanel.d.ts",
      "import": "./dist/ChartPanel.svelte"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@ticatec/i18n": "^0.2.0",
    "publint": "^0.3.2",
    "sass": "^1.57.1",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.3.1",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "@sveltejs/adapter-static": "^3.0.8",
    "echarts": "^6.0.0"
  },
  "author": {
    "name": "Henry Feng",
    "email": "henry@ticatec.com"
  },
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
  }
}